{
    "name": "@kreskolabs/protocol",
    "description": "Kresko solidity sources",
    "version": "1.0.0",
    "author": {
        "name": "Kresko Labs",
        "url": "https://kresko.fi"
    },
    "devDependencies": {
        "@codechecks/client": "^0.1.10",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@defi-wonderland/smock": "^2.0.7",
        "@ethersproject/abi": "^5.6.3",
        "@ethersproject/abstract-signer": "^5.6.2",
        "@ethersproject/bignumber": "^5.6.2",
        "@ethersproject/bytes": "^5.6.1",
        "@ethersproject/contracts": "^5.6.2",
        "@ethersproject/providers": "^5.6.8",
        "@ethersproject/solidity": "^5.6.1",
        "@kreskolabs/gnosis-safe-contracts": "1.3.3",
        "@kreskolabs/hardhat-deploy": "^2.2.0",
        "@kreskolabs/openzeppelin-diamond": "^2.0.0",
        "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
        "@nomiclabs/hardhat-etherscan": "^3.0.3",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@openzeppelin/contracts": "^4.6.0",
        "@openzeppelin/contracts-upgradeable": "^4.6.0",
        "@openzeppelin/test-helpers": "^0.5.15",
        "@rari-capital/solmate": "^6.4.0",
        "@solidity-parser/parser": "^0.14.2",
        "@solidstate/hardhat-4byte-uploader": "^1.0.2",
        "@sushiswap/core": "^1.4.2",
        "@sushiswap/core-sdk": "^1.0.0-canary.34",
        "@typechain/ethers-v5": "10.1.0",
        "@typechain/hardhat": "^6.1.2",
        "@types/chai": "^4.3.1",
        "@types/fs-extra": "^9.0.11",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "chai": "^4.3.6",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "^10.0.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "ethereum-waffle": "^3.4.4",
        "ethers": "^5.6.8",
        "fs-extra": "^10.0.0",
        "hardhat": "2.9.9",
        "hardhat-contract-sizer": "^2.5.1",
        "hardhat-diamond-abi": "^3.0.0",
        "hardhat-gas-reporter": "^1.0.8",
        "hardhat-interface-generator": "^0.0.6",
        "hardhat-preprocessor": "^0.1.4",
        "hardhat-watcher": "^2.3.0",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "mocha": "^10.0.0",
        "module-alias": "^2.2.2",
        "prettier": "^2.6.2",
        "prettier-plugin-solidity": "git+ssh://git@github.com/prettier-solidity/prettier-plugin-solidity",
        "shelljs": "^0.8.4",
        "solhint": "3.3.7",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.7.21",
        "ts-essentials": "^9.1.2",
        "ts-generator": "^0.1.1",
        "ts-node": "^10.8.0",
        "tsc": "^2.0.4",
        "tsconfig-paths": "^4.0.0",
        "typechain": "^8.1.0",
        "typescript": "^4.7.4",
        "web3": "^1.7.4"
    },
    "keywords": [
        "blockchain",
        "ethereum",
        "hardhat",
        "smart-contracts",
        "solidity"
    ],
    "license": "WTFPL",
    "private": true,
    "resolutions": {
        "@ethersproject/wallet": "^5.2.0"
    },
    "scripts": {
        "clean": "TS_NODE_TRANSPILE_ONLY=1 hardhat clean",
        "commit": "git-cz",
        "recompile": "TS_NODE_TRANSPILE_ONLY=1 hardhat clean && TS_NODE_TRANSPILE_ONLY=1 hardhat compile",
        "compile": "TS_NODE_TRANSPILE_ONLY=1 hardhat compile",
        "interface": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat gen-interface",
        "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
        "dev": "hardhat node",
        "deploy": "hardhat deploy",
        "deploy:kovan": "hardhat deploy --network kovan",
        "deploy:auroratest": "hardhat deploy --network auroratest",
        "deploy:aurora": "hardhat deploy --network aurora",
        "deployone:kresko": "hardhat deploy:kresko",
        "deployone:krasset": "hardhat deploy:kresko",
        "deployone:fluxpricefeed": "hardhat deployone:fluxpricefeed",
        "deployone:fluxaggregator": "hardhat deployone:fluxpriceaggregator",
        "deployone:fluxregistry": "hardhat deployone:feedsregistry",
        "upgrade:kresko": "hardhat upgrade:kresko",
        "upgrade:krasset": "hardhat upgrade:krasset",
        "kresko:addkrasset": "hardhat kresko:addkrasset",
        "kresko:addcollateral": "hardhat kresko:addcollateral",
        "lint": "yarn run lint:sol && yarn run lint:ts && yarn run prettier:check",
        "lint:sol": "TS_NODE_TRANSPILE_ONLY=1 solhint --config .solhint.json --max-warnings 0 './src/contracts/**/**/*.sol'",
        "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
        "postinstall": "husky install",
        "prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
        "prettier:check": "prettier --check --config ./.prettierrc \"**/*.{js,json,md,sol,ts}\"",
        "test": "TEST=true hardhat test",
        "typechain": "hardhat typechain",
        "publish:contracts": "cd packages/contracts && npm run clean && cd ../.. && hardhat export --export-all packages/contracts/deployments/deployments.json && cp -a types/contracts packages/contracts/types && cd packages/contracts && npm run build && npm publish"
    },
    "workspaces": [
        "packages/contracts"
    ]
}
