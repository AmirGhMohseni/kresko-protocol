{
    "solidity-va.parser.parseImports": true,
    "solidity-va.outline.decorations": true,
    "solidity-va.whatsNew.disabled": false,
    "solidity-va.codelens.ftrace.enable": true,
    "solidity-va.preview.markdown": true,
    "solidity-va.preview.dot": false,
    "solidity-va.diagnostics.cdili_json.import": true,
    "solidity-va.deco.statevars": false,
    "solidity-va.deco.arguments": false,
    "mochaExplorer.require": ["ts-node/register", "tsconfig-paths/register"],
    "mochaExplorer.files": ["./src/test/**/*.ts"],
    "mochaExplorer.timeout": 120000,
    "mochaExplorer.multiFileSuites": true,
    "files.exclude": {
        ".husky": true,
        ".gitattributes": true,
        ".DS_Store": true,
        ".commitlintrc.js": true,
        ".env.example": true,
        ".eslintignore": true,
        ".eslintrc.yaml": true,
        ".gitignore": true,
        ".lintstagedrc": true,
        ".prettierignore": true,
        ".solcover.js": true,
        ".solhint.json": true,
        ".solhintignore": true,
        ".yarnrc.yml": true,
        "LICENSE.md": true,
        "README.md": true,
        ".prettierrc": true,
        ".czrc": true,
        ".editorconfig": true
    },
    "solidity-va.hover": true,
    "solidity-va.codelens.enable": true
}
